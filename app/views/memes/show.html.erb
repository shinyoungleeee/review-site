<div class="row text-center">
  <h1><%= @meme.name %></h1>
</div>
<div class="row text-center">
  <img src=<%= @meme.image_url %> alt=<%= @meme.name %> class="thumbnail">
</div>
<div class="row text-center">
  <p><%= @meme.description %></p>
</div>

<%= link_to 'Delete Meme', @meme, method: :delete, data: {confirm: "Are you sure?"} %>
