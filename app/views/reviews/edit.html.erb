<h1>Edit Review!</h1>
<%= form_for [@meme, @review] do |f| %>
  <div class="field">
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  </div>
  <div class="field">
  <%= f.label :body %>
  <%= f.text_field :body %>
  </div>
  <div class="actions">
    <%= f.submit "Update Review", class: "button"%>
  </div>
<% end %>
